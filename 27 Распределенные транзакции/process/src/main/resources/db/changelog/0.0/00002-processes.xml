<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet id="00001-processes" author="tikskit">
        <preConditions>
            <not>
                <tableExists tableName="processes" schemaName="processes"/>
            </not>
        </preConditions>
        <createTable tableName="processes" schemaName="processes">
            <column name="processid" type="varchar(36)">
                <constraints primaryKey="true" primaryKeyName="pk_processes"/>
            </column>
            <column name="params" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="state" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="prescription_step" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="doctor_step" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="payment_step" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="notification_step" type="varchar">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>