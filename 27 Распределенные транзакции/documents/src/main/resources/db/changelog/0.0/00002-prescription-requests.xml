<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet id="00001-prescription-requests" author="tikskit">
        <preConditions>
            <not>
                <tableExists tableName="prescription_requests" schemaName="documents"/>
            </not>
        </preConditions>
        <createTable tableName="prescription_requests" schemaName="documents">
            <column name="prescriptionid" type="varchar(36)">
                <constraints primaryKey="true" primaryKeyName="pk_doctor_requests"/>
            </column>
            <column name="patientid" type="varchar(36)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>